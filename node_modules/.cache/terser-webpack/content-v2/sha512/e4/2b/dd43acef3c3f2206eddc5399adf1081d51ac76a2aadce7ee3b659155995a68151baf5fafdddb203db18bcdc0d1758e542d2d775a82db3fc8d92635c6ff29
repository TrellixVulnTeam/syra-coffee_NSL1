             break;\n                case 'imported':\n                    ans = ispayment ? this.payment_data.map((t) => t.total_payable).reduce((acc, value) => acc + value, 0) : this.iva_data.map((t) => Number(t.total_with_tax)).reduce((acc, value) => acc + value, 0);\n                    break;\n                default:\n                    break;\n            }\n            return ans;\n        }\n    }\n    AccountingReportComponent.ɵfac = function AccountingReportComponent_Factory(t) { return new (t || AccountingReportComponent)(core[\"Pb\" /* ɵɵdirectiveInject */](common_service[\"a\" /* CommonService */]), core[\"Pb\" /* ɵɵdirectiveInject */](api_services[\"a\" /* APIServices */]), core[\"Pb\" /* ɵɵdirectiveInject */](ng_bootstrap[\"c\" /* NgbModal */])); };\n    AccountingReportComponent.ɵcmp = core[\"Jb\" /* ɵɵdefineComponent */]({ type: AccountingReportComponent, selectors: [[\"app-accounting-report\"]], viewQuery: function AccountingReportComponent_Query(rf, ctx) {\n            if (rf & 1) {\n                core[\"Rc\" /* ɵɵviewQuery */](_c0, true);\n                core[\"Rc\" /* ɵɵviewQuery */](_c1, true);\n            }\n            if (rf & 2) {\n                let _t;\n                core[\"wc\" /* ɵɵqueryRefresh */](_t = core[\"hc\" /* ɵɵloadQuery */]()) && (ctx.vatChart = _t.first);\n                core[\"wc\" /* ɵɵqueryRefresh */](_t = core[\"hc\" /* ɵɵloadQuery */]()) && (ctx.Payoutchart = _t.first);\n            }\n        }, decls: 34, vars: 11, consts: [[1, \"container\", \"wrapper\"], [1, \"row\", \"mx-4\"], [1, \"col-12\", \"h-100\", \"col-12\", \"px-0\"], [1, \"col-12\"], [1, \"row\", \"justify-content-end\"], [1, \"border-0\", \"text-white\", \"exportBtn\", \"shadow-sm\", \"rounded-sm\"], [1, \"pr-2\", \"text-white\", 3, \"icon\"], [1, \"border-0\", \"text-white\", \"ml-4\", \"exportBtn\", \"mw-100x\", \"shadow-sm\", \"rounded-sm\", 3, \"click\"], [\"alt\", \"\", \"width\", \"100\", \"class\", \"position-absolute\", \"style\", \"right